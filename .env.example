# .env.example
# Snowflake Source Account
SOURCE_SNOWFLAKE_USER=your_source_user
SOURCE_SNOWFLAKE_PASSWORD=your_source_password
SOURCE_SNOWFLAKE_ACCOUNT=your_source_account.snowflakecomputing.com
SOURCE_SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SOURCE_SNOWFLAKE_DATABASE=PROD_DB
SOURCE_SNOWFLAKE_SCHEMA=PUBLIC
SOURCE_SNOWFLAKE_ROLE=ANALYST_ROLE

# Snowflake Target Account
TARGET_SNOWFLAKE_USER=your_target_user
TARGET_SNOWFLAKE_PASSWORD=your_target_password
TARGET_SNOWFLAKE_ACCOUNT=your_target_account.snowflakecomputing.com
TARGET_SNOWFLAKE_WAREHOUSE=COMPUTE_WH
TARGET_SNOWFLAKE_DATABASE=PROD_DB
TARGET_SNOWFLAKE_SCHEMA=PUBLIC
TARGET_SNOWFLAKE_ROLE=ANALYST_ROLE

# Optional: Email notifications
SMTP_SERVER=smtp.yourorg.com
SMTP_PORT=587
SMTP_USERNAME=notifications@yourorg.com
SMTP_PASSWORD=your_smtp_password
NOTIFICATION_RECIPIENTS=data-team@yourorg.com,migration-team@yourorg.com

# Optional: Slack notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
